@model IEnumerable<BilalEADProject.Models.Customer>
@{
    ViewBag.Title = "ViewAll";
}

<h2>ViewAll</h2>
<table>

    <tr>
        <th>ID</th>
        <th>Name</th>
    </tr>

    @{
        foreach (var s in Model)
        {
            <tr>
                <td>@s.Id</td>
                <td>@s.UserName</td>

            </tr>
        }
    }
</table>
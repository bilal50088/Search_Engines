@model IEnumerable<BilalEADProject.Models.Posts>
@{
    ViewBag.Title = "Viewpendingpost";
}
<table>

    <tr>
        <th>ID</th>
        <th>Model</th>
        <th>Make</th>
        <th>Year</th>
        <th>Engine</th>
        <th>Registration</th>
        <th>Price</th>
        <th>Location</th>
        <th>Type</th>
        <th>Status</th>
        <th>CID</th>
     
    </tr>

    @{
        foreach (var s in Model)
        {
            <tr>
                <td>@s.Id</td>
                <td>@s.Make</td>
                <td>@s.Make</td>
                <td>@s.Year</td>
                <td>@s.Engine</td>
                <td>@s.Registration</td>
                <td>@s.Price</td>
                <td>@s.Location</td>
                <td>@s.Type</td>
                <td>@s.Status</td>
                <td>@s.CID</td>



               

            </tr>
        }
    }
</table>